{
  "name": "react-redux-mocha-enzyme-starterkit",
  "version": "0.2.1",
  "description": "Boilerplate with latest react redux mocha and enzyme",
  "author": "devu",
  "license": "MIT",
  "scripts": {
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run serve:src\" \"npm run lint\"",
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/devServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/prodServer.js",
    "clean:dist": "rimraf ./dist",
    "lint": "eslint ./src ./tools ./config \"./*.js\"",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint --fix ./src ./tools ./config \"./*.js\"",
    "test": "cross-env NODE_PATH=./src mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.test.js\"",
    "test:coverage": "cross-env NODE_PATH=./src nyc --reporter=lcov mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "run-p serve:src test:watch",
    "prebuild": "run-s clean:dist test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "list-browsers": "babel-node ./tools/printBrowsersList",
    "precommit": "lint-staged",
    "postcommit": "git update-index -g"
  },
  "pre-commit": [
    "precommit",
    "test"
  ],
  "lint-staged": {
    "*{.js,.jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "es6-promise": "^4.2.8",
    "history": "4.7.2",
    "husky": "0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "15.6.1",
    "raf": "^3.4.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "4.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-logger": "^3.0.6",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/generator": "^7.3.3",
    "autoprefixer": "8.6.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.26.0",
    "braces": "^3.0.2",
    "browser-sync": "^2.26.7",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "compression-webpack-plugin": "1.1.11",
    "concurrently": "^3.5.1",
    "cross-env": "5.1.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jsdom": "11.11.0",
    "lint-staged": "7.1.3",
