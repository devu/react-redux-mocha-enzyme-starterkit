{
  "name": "webpack-react-redux-starter",
  "version": "0.2.1",
  "description": "Modern starter kit for React projects.",
  "author": "Aliaksandr Stsiarzhanau <sterzhanoff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run serve:src\" \"npm run lint\"",
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/devServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/prodServer.js",
    "clean:dist": "rimraf ./dist",
    "lint": "eslint ./src ./tools ./config \"./*.js\"",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint --fix ./src ./tools ./config \"./*.js\"",
    "test": "cross-env NODE_PATH=./src mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "run-p serve:src test:watch",
    "prebuild": "run-s clean:dist test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "list-browsers": "babel-node ./tools/printBrowsersList",
    "precommit": "lint-staged",
    "postcommit": "git update-index -g"
  },
  "lint-staged": {
    "*{.js,.jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@pearson-components/elements-sdk": "^1.12.7",
    "@pearson-components/pagination": "^1.1.0",
    "classnames": "2.2.6",
    "enzyme": "3.3.0",
    "es6-promise": "^4.2.4",
    "history": "4.7.2",
    "husky": "0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "15.6.1",
    "raf": "^3.4.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-hot-loader": "4.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "autoprefixer": "8.6.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.24.4",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "compression-webpack-plugin": "1.1.11",
    "concurrently": "^3.5.1",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.1",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "imagemin-webpack-plugin": "2.1.5",
    "jsdom": "11.11.0",
    "lint-staged": "7.1.3",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "4.0.2",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "react-test-renderer": "16.4.0",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "5.1.1",
    "sinon-chai": "3.1.0",
    "style-loader": "0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "svg-sprite-loader": "3.8.0",
    "svgo": "1.0.5",
    "svgo-loader": "2.1.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "keywords": [
    "es6",
    "react",
    "react 16"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stsiarzhanau/webpack-react-redux-starter.git"
  }
}
